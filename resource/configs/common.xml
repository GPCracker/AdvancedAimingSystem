<root>
	<!-- Target scanner. -->
	<!-- Сканер цели. -->
	<targetScanner>
		<enabled>True</enabled>
		<!-- Target scan mode. -->
		<!-- Режим сканирования. -->
		<scanMode>
			<!-- Enables normal target to be accounted. -->
			<!-- Разрешает использование обычного режима сканирования. -->
			<useStandardMode>True</useStandardMode>
			<!-- Enables XRay target scan module to be used. -->
			<!-- Разрешает использование рентгена для поиска целей. -->
			<useXRayMode>False</useXRayMode>
			<!-- Enables BoundingBox target scan module to be used. -->
			<!-- Разрешает использование BoundingBox сканера для поиска целей. -->
			<useBBoxMode>False</useBBoxMode>
			<!-- Enables BoundingEllipse target scan module to be used. -->
			<!-- Разрешает использование BoundingEllipse сканера для поиска целей. -->
			<useBEpsMode>False</useBEpsMode>
			<!-- Maximum target distance. Not affects on Normal scanner. -->
			<!-- Максимальная дистанция сканирования. Не влияет на обычный режим сканирования. -->
			<maxDistance>720.0</maxDistance>
			<!-- Bounds multiplier for Bounding scanners. -->
			<!-- Множитель размера для Bounding сканеров. -->
			<boundsScalar>2.0</boundsScalar>
			<!-- Target auto scan interval. -->
			<!-- Интервал автоматического сканирования цели. -->
			<autoScanInterval>0.04</autoScanInterval>
			<!-- When this time passes since target last time under crosshairs, target (auto scanned) is deprecated. -->
			<!-- Время устаревания цели с момента ее последнего попадания в прицел (для автоматического сканера). -->
			<autoScanExpiryTime>5.0</autoScanExpiryTime>
		</scanMode>
		<!-- Automatic scanner settings. -->
		<!-- Настройки автоматического сканера. -->
		<autoScan>
			<enabled>True</enabled>
			<activated>True</activated>
			<shortcut>
				<sequence>KEY_LALT+KEY_N</sequence>
				<switch>True</switch>
				<invert>False</invert>
			</shortcut>
			<message>
				<onActivate>#AdvancedAimingSystem:targetScanner/autoScan/onActivate;</onActivate>
				<onDeactivate>#AdvancedAimingSystem:targetScanner/autoScan/onDeactivate;</onDeactivate>
			</message>
		</autoScan>
		<!-- Manual target override. It will never be deprecated and could be changed or dropped only by player. -->
		<!-- Ручная перегрузка цели. Цель не устаревает и может быть изменена или сброшена только игроком. -->
		<manualOverride>
			<enabled>False</enabled>
			<shortcut>KEY_T</shortcut>
		</manualOverride>
	</targetScanner>
	<!-- Safe shooting settings. -->
	<!-- Настройки безопасной стрельбы. -->
	<safeShot>
		<enabled>True</enabled>
		<activated>True</activated>
		<shortcut>
			<sequence>KEY_LALT</sequence>
			<switch>False</switch>
			<invert>True</invert>
		</shortcut>
		<message>
			<onActivate>#AdvancedAimingSystem:safeShot/onActivate;</onActivate>
			<onDeactivate>#AdvancedAimingSystem:safeShot/onDeactivate;</onDeactivate>
		</message>
		<!-- Enables gun target (vehicle your gun is aimed at) usage. The most effective solution for preventing friendly fire. -->
		<!-- Разрешает использование цели, на которую наведено орудие. Наиболее эффективное средство защиты от дружественного огня. -->
		<useGunTarget>True</useGunTarget>
		<!-- Enables friendly fire on team killers, considering them hostiles. -->
		<!-- Разрешает огонь по "голубкам", считая их враждебными. -->
		<considerBlueHostile>False</considerBlueHostile>
		<!-- Corpse is considered fresh for this time after it dies. -->
		<!-- Труп считается свежим в течение этого времени после смерти. -->
		<fragExpirationTimeout>2.0</fragExpirationTimeout>
		<!-- Vehicle error message template. -->
		<!-- Шаблон сообщения об ошибке. -->
		<template>#AdvancedAimingSystem:safeShot/error/template;</template>
		<!-- Shot blocking reasons. -->
		<!-- Причины блокировки выстрела. -->
		<reasons>
			<!-- Friendly fire. -->
			<!-- Дружественный огонь. -->
			<team>
				<enabled>True</enabled>
				<!-- Team chat messaging settings. -->
				<!-- Настройки отправки сообщений в командный чат. -->
				<chat>
					<enabled>True</enabled>
					<message>#AdvancedAimingSystem:safeShot/reasons/team/chat/message;</message>
				</chat>
				<template>#AdvancedAimingSystem:safeShot/reasons/team/template;</template>
			</team>
			<!-- Corpse shooting. -->
			<!-- Стрельба по трупам. -->
			<dead>
				<enabled>True</enabled>
				<template>#AdvancedAimingSystem:safeShot/reasons/dead/template;</template>
			</dead>
			<!-- Useless shooting (shooting deliberately off the target). -->
			<!-- Бесполезная стрельба (стрельба заведомо мимо цели). -->
			<waste>
				<enabled>False</enabled>
				<template>#AdvancedAimingSystem:safeShot/reasons/waste/template;</template>
			</waste>
		</reasons>
	</safeShot>
	<!-- Sniper mode for artillery. -->
	<!-- Снайперский режим для артиллерии. -->
	<sniperModeSPG>
		<enabled>False</enabled>
		<shortcut>KEY_E</shortcut>
	</sniperModeSPG>
	<!-- Automatic aim settings. -->
	<!-- Настройки автоприцела. -->
	<autoAim>
		<!-- Enables real time scanning result substitution while locking on target. -->
		<!-- Разрешает подстановку результата сканирования в реальном времени при захвате цели. -->
		<useTargetScan>False</useTargetScan>
		<!-- Enables TargetInfo substitution while locking on target. -->
		<!-- Разрешает подстановку TargetInfo при захвате цели. -->
		<useTargetInfo>False</useTargetInfo>
	</autoAim>
	<!-- Radial menu settings. -->
	<!-- Настройки радиального меню. -->
	<radialMenu>
		<!-- Enables real time scanning result substitution while entering radial menu. -->
		<!-- Разрешает подстановку результата сканирования в реальном времени при входе радиальном меню. -->
		<useTargetScan>False</useTargetScan>
		<!-- Enables TargetInfo substitution while entering radial menu. -->
		<!-- Разрешает подстановку TargetInfo при входе радиальном меню. -->
		<useTargetInfo>False</useTargetInfo>
	</radialMenu>
	<!-- Expert perk settings. -->
	<!-- Настройки перка "Эксперт". -->
	<expertPerk>
		<enabled>False</enabled>
		<cacheExtrasInfo>True</cacheExtrasInfo>
		<replyTimeout>5.0</replyTimeout>
		<cacheExpiryTime>60.0</cacheExpiryTime>
	</expertPerk>
</root>

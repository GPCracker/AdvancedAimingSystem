<root>
	<enabled>True</enabled>
	<!-- Interface update interval. -->
	<!-- Интервал обновления интерфейса. -->
	<updateInterval>0.04</updateInterval>
	<!-- Info panels settings. -->
	<!-- Настройки информационных панелей. -->
	<panels>
		<!-- Static settings (could be changed only by user). -->
		<!-- Статические настройки (могут быть изменены только пользователем). -->
		<static>
			<!-- Aiming correction GUI settings. -->
			<!-- Настройки графического интерфейса корректировки прицеливания. -->
			<AdvancedAimingSystemCorrectionPanel>
				<!-- Default (all available) parameters of GUI item loaded as default after population. -->
				<!-- Параметры по умолчанию (все доступные) для графического элемента, загружаемые после отображения элемента. -->
				<default>
					<alpha>1.0</alpha>
					<visible>True</visible>
					<background></background>
					<tooltip>#AdvancedAimingSystem:gui/CorrectionPanel/default/tooltip;</tooltip>
					<template>#AdvancedAimingSystem:gui/CorrectionPanel/default/text;</template>
					<position>0.0 0.3</position>
					<size>450.0 25.0</size>
				</default>
				<!-- Parameters are applied over current state when entering appropriate aiming mode. When exiting "visible" is set to False. -->
				<!-- Warning: All aim mode sections should have same parameter set because they are applied over current parameters. -->
				<!-- Warning: In case of show-hide algorithm particular qualities "visible" parameter must be specified anyway. -->
				<!-- Warning: Parameters in "arcade" aim mode section should have same values as in "default" section.  -->
				<!-- You can add your parameters here, all available parameters are specified in "default" section. -->
				<!-- Additional information on this parameters is available in official topic. -->
				<!-- Параметры в данных секциях применяются при входе в соответствующий режим поверх текущего состояния. -->
				<!-- При выходе из режима параметр "visible" автоматически устанавливается в значение False. -->
				<!-- Внимание: Все секции режимов прицеливания должны иметь одинаковый набор параметров. -->
				<!-- Внимание: Параметр "visible" является обязательным и должен быть указан в любом случае. -->
				<!-- Внимание: Параметры для режима "arcade" должны иметь одинаковые значения с теми, что определены в секции "default". -->
				<!-- Вы можете добавлять параметры в секции режимов прицеливания, все доступные определены в секции "default". -->
				<!-- Дополнительная информация по данным параметрам доступна в официальной теме на форуме. -->
				<arcade>
					<visible>True</visible>
				</arcade>
				<sniper>
					<visible>True</visible>
				</sniper>
				<strategic>
					<visible>True</visible>
				</strategic>
			</AdvancedAimingSystemCorrectionPanel>
			<!-- Target scanner GUI settings. -->
			<!-- Настройки графического интерфейса сканера целей. -->
			<AdvancedAimingSystemTargetPanel>
				<default>
					<alpha>1.0</alpha>
					<visible>True</visible>
					<background></background>
					<tooltip>#AdvancedAimingSystem:gui/TargetPanel/default/tooltip;</tooltip>
					<template>#AdvancedAimingSystem:gui/TargetPanel/default/text;</template>
					<position>0.0 0.4</position>
					<size>450.0 25.0</size>
				</default>
				<arcade>
					<visible>True</visible>
				</arcade>
				<sniper>
					<visible>True</visible>
				</sniper>
				<strategic>
					<visible>True</visible>
				</strategic>
			</AdvancedAimingSystemTargetPanel>
			<!-- Aiming info GUI settings. -->
			<!-- Настройки графического интерфейса информации о сведении. -->
			<AdvancedAimingSystemAimingPanel>
				<default>
					<alpha>1.0</alpha>
					<visible>True</visible>
					<background>img://mods/GPCracker.AdvancedAimingSystem/icons/AimingInfoBackground.png</background>
					<tooltip>#AdvancedAimingSystem:gui/AimingPanel/default/tooltip;</tooltip>
					<template>#AdvancedAimingSystem:gui/AimingPanel/default/text;</template>
					<position>0.4 -0.1</position>
					<size>200.0 130.0</size>
				</default>
				<arcade>
					<visible>True</visible>
					<template>#AdvancedAimingSystem:gui/AimingPanel/arcade/text;</template>
					<position>0.4 -0.1</position>
				</arcade>
				<sniper>
					<visible>True</visible>
					<template>#AdvancedAimingSystem:gui/AimingPanel/sniper/text;</template>
					<position>0.4 -0.25</position>
				</sniper>
				<strategic>
					<visible>True</visible>
					<template>#AdvancedAimingSystem:gui/AimingPanel/strategic/text;</template>
					<position>-0.3 -0.4</position>
				</strategic>
			</AdvancedAimingSystemAimingPanel>
		</static>
		<!-- Dynamic settings (could be changed ingame by scripts to save panels movements, etc.). -->
		<!-- For preserving comments in user-only config files this section should be remapped to machine-only file. -->
		<!-- Динамические настройки (могут быть изменены скриптами для сохранения перемещения панелей и т.д.). -->
		<!-- В целях сохранения комментариев в конфигурационных файлах данную секцию следует переназначать на специальный файл конфигурации. -->
		<ingame override="../mods/configs/GPCracker.AdvancedAimingSystem/ingame.xml/gui/panels" />
	</panels>
</root>
